{
  "expo": {
    "name": "De-Suung Scanner",
    "slug": "desuung-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "userInterfaceStyle": "automatic"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.atbitchmdagunner.dhqscanner",
      "config": {
        "cleartext": {
          "usesCleartextTraffic": true
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ee1f8c9a-f04e-40ba-988a-be7fb543c8e3"
      }
    },
    "owner": "atbitchmdagunner"
  }
}
